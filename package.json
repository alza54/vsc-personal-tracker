{
  "publisher": "vsc-personal-tracker",
  "name": "personal-tracker",
  "displayName": "Easy Personal Tracker",
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "description": "Visual Studio Code personal addon created to help me tracking my work progresses",
  "author": {
    "name": "alza54",
    "url": "https://github.com/alza54",
    "email": "mateusz@vivant.pl"
  },
  "license": "MIT",
  "homepage": "https://github.com/alza54/vsc-personal-tracker#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/alza54/vsc-personal-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/alza54/vsc-personal-tracker/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "esno": "^4.0.0",
    "pnpm": "^8.15.4",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  }
}
